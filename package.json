{
  "name": "chitu",
  "amdName": "chitu",
  "globalName": "chitu",
  "version": "0.0.1",
  "description": "Chuck Her In The Ute",
  "main": "dist/chitu.js",
  "jsnext:main": "src/index.js",
  "minified:main": "dist/chitu.min.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm-run-all clean transpile minify size",
    "transpile": "rollup -c rollup.config.js -m ${npm_package_main}.map -f umd -n $npm_package_globalName src/index.js -o $npm_package_main",
    "minify": "uglifyjs $npm_package_main --mangle-props -cm -o $npm_package_minified_main -p relative --in-source-map ${npm_package_main}.map --source-map ${npm_package_minified_main}.map --screw-ie8",
    "size": "size=$(gzip-size $npm_package_minified_main) && echo \"gzip size: $(pretty-bytes $size)\"",
    "test": "npm-run-all lint test:ava",
    "test:ava": "ava --verbose",
    "test:watch": "npm run test:ava -- --watch",
    "test:ci": "nyc ava && nyc report --reporter=lcov | codecov",
    "test:report": "nyc ava && nyc report --reporter=html && open ./coverage/index.html",
    "lint": "eslint src test",
    "docs": "jsdoc --configure .jsdoc.json --verbose",
    "docs:open": "npm run docs && open docs/$npm_package_name/$npm_package_version/index.html",
    "gh-pages": "npm run docs && github-pages -r tymondesigns/chitu ./docs/$npm_package_name/$npm_package_version",
    "prepublish": "npm run build",
    "release": "NODE_ENV=production npm run build && npm test && git add -A && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tymondesigns/chitu.git"
  },
  "author": "Sean Tymon <tymon148@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tymondesigns/chitu/issues"
  },
  "homepage": "https://github.com/tymondesigns/chitu",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.2.0",
    "github-pages": "^3.0.2",
    "gzip-size-cli": "^1.0.0",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "npm-run-all": "^1.5.1",
    "nyc": "^5.6.0",
    "open": "0.0.5",
    "pretty-bytes-cli": "^1.0.0",
    "rimraf": "^2.5.1",
    "rollup": "^0.26.2",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-node-resolve": "^1.4.0",
    "uglify-js": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      [
        "typecheck",
        {
          "disable": {
            "production": true
          }
        }
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  }
}
